extends auth-base

block vars
  - var title = 'Dashboard'

block body
  h1 Dashboard
  if user
    a(href='/users/logout') Logout
    p.
      Welcome to your dashboard! You are now logged in.
    h2 User Information
    p First Name: #{user.firstName}
    p Last Name:  #{user.lastName}
    p Email:      #{user.email}
    p Data:       #{user.data}